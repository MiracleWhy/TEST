<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.uton.carsokApi.dao.NoticeMapper">
	
	<insert id="insert" parameterType="com.uton.carsokApi.model.Notice">
		INSERT INTO carsok_notice(
			id
			,table_name
			,record_id
			,status
			,create_time
			,update_time
		) VALUES (
			#{id},
			#{table_name},
			#{record_id},
			#{status},
			#{create_time},
			#{update_time}
		) 
	</insert>
</mapper>